                    <titleInfo>
                        <title>Biografisierung der Supervision und Wandel der Nachfrage oder wohin geht die DGSv</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Katharina</namePart>
                        <namePart type="family">Gröning</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
