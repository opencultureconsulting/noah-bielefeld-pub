                    <titleInfo>
                        <title>Amalgamating players, symmetry, and the Banzhaf value</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Andr√©</namePart>
                        <namePart type="family">Casajus</namePart>
