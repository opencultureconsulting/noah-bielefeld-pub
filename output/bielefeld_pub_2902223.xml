                    <titleInfo>
                        <title>On preemption in discrete and continuous time</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Jan-Henrik</namePart>
                        <namePart type="family">Steg</namePart>
                        <role>
