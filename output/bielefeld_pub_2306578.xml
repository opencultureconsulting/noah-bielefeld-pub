                    <titleInfo>
                        <title>Java-Backend für den ADP-Compiler</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Georg</namePart>
                        <namePart type="family">Sauthoff</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
