                        </role>
                    </name>
                    <name type="personal">
                        <namePart type="given">H.</namePart>
                        <namePart type="family">Warnecke</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">article</genre>
