                    <titleInfo>
                        <title>Coordination of the legs of a slow-walking cat</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Holk</namePart>
                        <namePart type="family">Cruse</namePart>
                        <role>
