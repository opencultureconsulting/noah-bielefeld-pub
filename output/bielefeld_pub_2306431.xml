                    <titleInfo>
                        <title>Mesoscopic simulations of electrohydrodynamic phenomena</title>
                    </titleInfo>
                    <titleInfo type="alternative">
                        <title>Mesoskopische Simulationen von elektrohydrodynamischen Phänomenen</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Jens</namePart>
                        <namePart type="family">Smiatek</namePart>
