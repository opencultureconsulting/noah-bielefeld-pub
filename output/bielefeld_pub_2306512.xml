                    <titleInfo>
                        <title>Evaluation unterschiedlicher Präsentationsformen im Rahmen einer Ausstellung zur Hominidenevolution : eine empirische Untersuchung zur Ausstellungsdidaktik</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Elke</namePart>
                        <namePart type="family">Möllmann</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
