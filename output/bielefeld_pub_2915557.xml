                    <titleInfo>
                        <title>Fiscal Sustainability: Does EU Membership Change Policy Behavior? Empirical Evidence from Central and Eastern Europe</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Bettina</namePart>
                        <namePart type="family">Fincke</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">workingPaper</genre>
                    <originInfo>
