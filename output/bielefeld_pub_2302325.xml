                    <titleInfo>
                        <title>Field theories for copolymer blends : self-consistent approaches and Monte Carlo simulations</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Dominik</namePart>
                        <namePart type="family">Düchs</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
