                    <titleInfo>
                        <title>Molekulare Hysterese und ihre kybernetische Bedeutung</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Eberhard</namePart>
                        <namePart type="family">Neumann</namePart>
                        <role>
