                        <topic>Fuzzy coalition</topic>
                        <topic>Stable set</topic>
                        <topic>Cooperative game</topic>
                        <topic>Core</topic>
                        <topic>Core stability</topic>
                        <topic>Fuzzy game</topic>
                    </subject>
