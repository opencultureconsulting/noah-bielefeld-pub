                    <titleInfo>
                        <title>Characterizing core stability with fuzzy games</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Evan</namePart>
                        <namePart type="family">Shellshear</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
