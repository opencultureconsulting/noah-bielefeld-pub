                    <titleInfo>
                        <title>Molecular Hysteresis and Its Cybernetic Significance</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Eberhard</namePart>
                        <namePart type="family">Neumann</namePart>
                        <role>
