                    <titleInfo>
                        <nonSort>The </nonSort>
                        <title>Transfer Principle holds for definable nonstandard models under Countable Choice</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Frederik</namePart>
                        <namePart type="family">Herzberg</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">workingPaper</genre>
