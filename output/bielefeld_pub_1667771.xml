                    <titleInfo>
                        <title>Multiple sequence alignment with the Divide-and-Conquer method.</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Jens</namePart>
                        <namePart type="family">Stoye</namePart>
                        <nameIdentifier type="orcid" typeURI="http://orcid.org">0000-0002-4656-7155</nameIdentifier>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
