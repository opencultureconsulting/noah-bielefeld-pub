                    <titleInfo>
                        <title>On Nonlinear Expectations and Markov Chains under Model Uncertainty</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Max</namePart>
                        <namePart type="family">Nendel</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
