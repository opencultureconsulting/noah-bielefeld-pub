                    <titleInfo>
                        <title>What mechanisms coordinate leg movement in walking arthropods?</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Holk</namePart>
                        <namePart type="family">Cruse</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">article</genre>
                    <originInfo>
