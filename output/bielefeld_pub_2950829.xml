                    <titleInfo>
                        <title>Higher Hölder regularity for nonlocal equations with irregular kernel</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Simon Noah</namePart>
                        <namePart type="family">Nowak</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">article</genre>
                    <originInfo>
