                    <titleInfo>
                        <title>Unparteilichkeit des Notars bei Tätigkeiten nach § 24 Abs. 1 BNotO?</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Andreas</namePart>
                        <namePart type="family">Bohnenkamp</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <name type="personal">
                        <namePart type="given">Fritz</namePart>
                        <namePart type="family">Jost</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">dgs</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">doctoralThesis</genre>
