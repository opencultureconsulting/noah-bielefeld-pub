                        <topic>Stereochemistry</topic>
                        <topic>Indanes</topic>
                        <topic>Indenes</topic>
                        <topic>Deuterium labelling</topic>
                        <topic>Homogeneous catalytic hydrogenation</topic>
                    </subject>
