                        <title>Note on Equity Premia in Markets with Heterogeneous Agents</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Tatjana</namePart>
                        <namePart type="family">Chudjakow</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
