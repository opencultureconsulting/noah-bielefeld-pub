                    <titleInfo>
                        <title>Interactions of Bunias orientalis plant chemotypes and fungal pathogens with different host specificity in vivo and in vitro</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Lisa-Johanna</namePart>
                        <namePart type="family">Tewes</namePart>
                        <role>
