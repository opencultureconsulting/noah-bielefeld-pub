                    <titleInfo>
                        <title>Characterization of time-consistent sets of measures in finite trees</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Monika</namePart>
                        <namePart type="family">Bier</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
