                    <titleInfo>
                        <title>Dynamic Consistency in Incomplete Information Games with Multiple Priors</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Marieke</namePart>
                        <namePart type="family">Pahlke</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
