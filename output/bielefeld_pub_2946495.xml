                        <topic>Finitely repeated games</topic>
                        <topic>Pure strategy</topic>
                        <topic>Observable mixed strategies</topic>
                        <topic>Subgame perfect Nash equilibrium</topic>
                        <topic>Limit perfect folk theorem</topic>
                    </subject>
