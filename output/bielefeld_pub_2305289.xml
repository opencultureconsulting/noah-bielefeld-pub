                    <titleInfo>
                        <title>Tuberculosis in Pakistan : improving control strategies</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Mohammed Ibrahim</namePart>
                        <namePart type="family">Khan</namePart>
                        <role>
