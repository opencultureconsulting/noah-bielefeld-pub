                    <titleInfo>
                        <title>PET-oxidative Silylenolethercyclisierung : mechanistische Untersuchungen und Steroidsynthese durch Dominoreaktionen</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Jens Otto</namePart>
                        <namePart type="family">Bunte</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
