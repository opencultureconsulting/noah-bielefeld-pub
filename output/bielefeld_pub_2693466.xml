                    <titleInfo>
                        <title>Geschlechtstypische Unterschiede in den Berufspräferenzen deutscher Jugendlicher</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Anna</namePart>
                        <namePart type="family">Karsch</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
                    <typeOfResource>text</typeOfResource>
                    <genre authority="dini">workingPaper</genre>
