                    <titleInfo>
                        <title>Femtosecond time-resolved photoelectron spectroscopy in the extreme ultraviolet region</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Peter</namePart>
                        <namePart type="family">Siffalovic</namePart>
