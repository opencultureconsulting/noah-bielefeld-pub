                    <titleInfo>
                        <title>Kuhn&apos;s Theorem for Extensive Form Ellsberg Games</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Linda</namePart>
                        <namePart type="family">Sass</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
