                    <titleInfo>
                        <title>Was soll ich wollen? Zur supervisorischen Relevanz der Frage nach dem guten Leben</title>
                    </titleInfo>
                    <name type="personal">
                        <namePart type="given">Volker</namePart>
                        <namePart type="family">Dieringer</namePart>
                        <role>
                            <roleTerm type="code" authority="marcrelator">aut</roleTerm>
                        </role>
                    </name>
