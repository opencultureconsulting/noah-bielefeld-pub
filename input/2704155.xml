        &lt;/tr&gt;
        &lt;tr&gt;
          &lt;td&gt;Duration:&lt;/td&gt;
          &lt;td&gt;1.9 s&lt;/td&gt;
        &lt;/tr&gt;
      &lt;/tbody&gt;
    &lt;/table&gt;&lt;br&gt;
